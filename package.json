{
  "name": "graph-engineering",
  "version": "0.0.0",
  "description": "User interface for creating and interacting with GraphQL schemas.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GraphEngineering/graph-engineering.git"
  },
  "scripts": {
    "clean": "rimraf build tmp .cache",
    "generate-schema-json": "apollo-codegen introspect-schema src/graphql/schema.graphql --output tmp/schema.json",
    "generate-code": "npm run generate-schema-json && apollo-codegen generate src/components/**/*.tsx --schema tmp/schema.json --target typescript --output src/types/generated/operations.ts",
    "dev": "parcel src/index.html",
    "start": "nodemon"
  },
  "author": "CryptoGraph",
  "license": "MIT",
  "dependencies": {
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.21.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "upgrade": "^1.1.0"
  },
  "devDependencies": {
    "@cryptograph/typescript-tooling": "^1.0.5",
    "@types/graphql": "^0.12.4",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.15",
    "apollo-codegen": "^0.18.6",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "clean": "^4.0.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1",
    "parcel-bundler": "^1.6.2",
    "postcss-modules": "^1.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.7.0",
    "typescript": "^2.7.1"
  }
}
